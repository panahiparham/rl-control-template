{
    "agent": "TD",
    "problem": "ShortRandChain",
    "total_steps": 10000,
    "episode_cutoff": -1,
    "metaParameters": {
        "epsilon": 0.1,
        "buffer_type": ["iid", "per", "pser", "backwards"],
        "buffer_size": 1000,
        "batch": 32,
        "n_step": 1,

        "optimizer": {
            "name": "ADAM",
            "alpha": 0.001,
            "beta1": 0.9,
            "beta2": 0.999
        },

        "representation": {
            "type": "TwoLayerRelu",
            "hidden": 8
        },

        "environment": {
            "features": "inverted"
        }
    }
}
